// .aa Dili Gelişmiş If-Else If-Else Testi

var not = 75
print("Ogrenci Notu:")
print(not)

if (not > 90) {
    print("Derece: AA")
} else {
    // Else içinde yeni bir IF (Else-If mantığı)
    if (not > 70) {
        print("Derece: BB")
    } else {
        if (not > 50) {
            print("Derece: CC")
        } else {
            print("Durum: Kaldiniz")
        }
    }
}

